{
  "name": "@uzmoi/ut",
  "version": "0.1.5",
  "exports": {
    "./encoding": "./src/encoding/index.ts",
    "./fp": "./src/fp/index.ts",
    "./ils": "./src/ils/index.ts",
    "./iter": "./src/iter/index.ts",
    "./types": "./src/types/index.ts"
  },
  "publish": {
    "include": [
      "src/**/*.ts",
      "LICENSE",
      "README.md",
      "deno.json",
      "package.json"
    ],
    "exclude": [
      "**/*.test.ts"
    ]
  },
  "tasks": {
    "cspell": "cspell",
    "build": "tsdown"
  },
  "license": "MIT",
  "imports": {
    "@std/assert": "jsr:@std/assert@1"
  },
  "exclude": ["dist/"],
  "compilerOptions": {
    "noUncheckedIndexedAccess": true,
    "exactOptionalPropertyTypes": true,
    "lib": ["esnext", "dom", "dom.iterable"]
  }
}
